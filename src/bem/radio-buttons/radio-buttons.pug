mixin radio-buttons(name, id1, value1, id2, value2, label1, label2)
   div(style!=attributes.margin).radio-buttons
      input(type="radio" id=id1 name=name value=value1 checked).radio-buttons__circle
      label(for=id1).radio-buttons__label=label1
      input(type="radio" id=id2 name=name value=value2).radio-buttons__circle
      label(for=id2).radio-buttons__label=label2